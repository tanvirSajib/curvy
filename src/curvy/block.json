{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "blockylicious/curvy",
	"version": "0.1.0",
	"title": "Curvy",
	"category": "text",
	"icon": "smiley",
	"description": "Curvy shaped dividers to make dividing page content more interesting.",
	"example": {},
	"supports": {
		"html": false,
		"color": {
			"text": true,
			"background": true,
			"link": true
		},
		"spacing": {
			"padding": true
		}
	},
	"textdomain": "blockylicious",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js",
	"attributes": {
		"style":{
			"type": "object",
			"default":{
				"color":{
					"background":"#ec4899"
				},
				"spacing":{
					"padding":{
						"top": "80px",
						"bottom": "80px",
						"left":"50px",
						"right":"50px"
					}
				}
			}
		},
		"enableTopCurve":{
			"type": "boolean",
			"default": true
		},
		"topWidth":{
			"type":"number",
			"default":100
		},
		"topHeight":{
			"type":"number",
			"default":100
		},
		"topFlipX":{
			"type":"boolean",
			"default":false
		},
		"topFlipY":{
			"type":"boolean",
			"default":false
		}
	}
	
}
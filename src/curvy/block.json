{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "blockylicious/curvy",
	"version": "0.1.0",
	"title": "Curvy",
	"category": "text",
	"icon": "smiley",
	"description": "Curvy shaped dividers to make dividing page content more interesting.",
	"example": {},
	"supports": {
		"html": false,
		"color": {
			"text": true,
			"background": true
		}
	},
	"textdomain": "blockylicious",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js",
	"attributes": {
		"enableTopCurve":{
			"type": "boolean",
			"default": true
		}
	}
}